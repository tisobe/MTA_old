<script type="text/javascript">


    function WindowOpener(file) {
        msgWindow = open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=no,status=no,width=720,height=800,resize=no");
        msgWindow.document.clear();
        msgWindow.document.write("<!DOCTYPE html><html><head><title>ACIS Histogram Plot Fitting Results</title></head>");
        msgWindow.document.write("<body bgcolor='white'>");
        msgWindow.document.write("<iframe src='https://cxc.cfa.harvard.edu/mta_days/mta_acis_hist/Fittings/"+file+"' border=0 width=720 height=800></iframe>")
        msgWindow.document.write("</body></html>")
        msgWindow.document.close();
        msgWindow.focus();
    }
</script>

